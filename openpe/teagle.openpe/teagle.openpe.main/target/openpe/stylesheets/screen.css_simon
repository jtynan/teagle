	}









	.submit.button {
		cursor: pointer;
		display: inline-block;
		margin: 0.6em 0.6em 0.8em 0;
		/*text-align: center;*/
	    font-weight: bold;	
	    border-width: 0 10px;
	    /*padding: 4px 4px 1px 4px;*/
